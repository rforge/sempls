S3method(print, summary.bootsempls)
S3method(pathDiagram, sempls)
S3method(plsm2sem, plsm)
S3method(rSquared, sempls)
S3method(densityplot, sempls)
S3method(densityplot, bootsempls)
S3method(parallelplot, bootsempls)
